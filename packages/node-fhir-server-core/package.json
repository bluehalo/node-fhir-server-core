{
  "name": "@projecttacoma/node-fhir-server-core",
  "version": "2.2.5",
  "description": "Node FHIR Rest API Server",
  "license": "MIT",
  "author": "Asymmetrik Ltd.",
  "contributors": [
    "Robert Winterbottom <rwinterbottom@asymmetrik.com>",
    "Jon Lee <jlee@asymmetrik.com>",
    "Sumeet Shah <sshah@asymmetrik.com>",
    "Shane O'Neill <soneill@asymmetrik.com>"
  ],
  "main": "dist/index",
  "scripts": {
    "build": "babel src -d dist --copy-files",
    "build:watch": "babel src -w -d dist --copy-files",
    "changelog": "conventional-changelog -p angular -s -i CHANGELOG.md",
    "lint": "eslint \"src/**/*.js\"",
    "nodemon": "cross-env NODE_ENV=development node scripts/nodemon.js",
    "prepublish": "yarn build",
    "snyk": "snyk",
    "snyk-protect": "snyk protect",
    "start": "node scripts/runner.js",
    "test": "cross-env NODE_ENV=test jest src"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/server/resources/"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "json"
    ],
    "verbose": true
  },
  "dependencies": {
    "@asymmetrik/fhir-response-util": "^1.2.4",
    "@asymmetrik/sof-scope-checker": "^1.0.7",
    "@hapi/bourne": "^1.3.2",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "eslint": "^6.1.0",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "helmet": "^3.9.0",
    "jsonschema": "^1.2.2",
    "jsonwebtoken": "^8.1.0",
    "jwk-to-pem": "^2.0.2",
    "method-override": "^2.3.10",
    "moment": "^2.19.4",
    "moment-timezone": "^0.5.14",
    "mongodb": "^3.0.0-rc0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "sanitize-html": "^2.5.1",
    "serve-favicon": "^2.5.0",
    "snyk": "^1.731.0",
    "superagent": "^3.8.3",
    "validator": "^9.1.2",
    "winston": "^3.2.1",
    "xss": "^0.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "conventional-changelog-cli": "^2.0.11",
    "nock": "^9.1.5",
    "nodemon": "^2.0.13",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || >=11.10.1"
  },
  "snyk": true,
  "gitHead": "3d472bc86858c277750e21d58052a4fef76dceae"
}
